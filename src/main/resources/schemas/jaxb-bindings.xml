<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
        version="2.1">

    <jxb:bindings schemaLocation="CREmployee.xsd">
        <jxb:bindings node="//xs:complexType[@name='CREmployeeType']">
            <jxb:class name="CREmployee"/>
        </jxb:bindings>

        <jxb:bindings node="//xs:complexType[@name='AdditionalInfoType']">
            <jxb:class name="AdditionalInfo"/>
        </jxb:bindings>
    </jxb:bindings>

</jxb:bindings>