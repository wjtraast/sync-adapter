<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
        version="2.1">

    <jxb:bindings schemaLocation="CREmployee.xsd">
        <jxb:bindings node="//xs:complexType[@name='CREmployeeType']">
            <jxb:class name="CREmployee"/>
        </jxb:bindings>

        <jxb:bindings node="//xs:complexType[@name='CRUserType']">
            <jxb:class name="CRUser"/>
        </jxb:bindings>

        <jxb:bindings node="//xs:complexType[@name='toUserType']">
            <jxb:class name="ToUser"/>
        </jxb:bindings>



        <jxb:bindings node="//xs:complexType[@name='toGenderNodeType']">
            <jxb:class name="ToGenderNode"/>
        </jxb:bindings>

        <jxb:bindings node="//xs:complexType[@name='CRDataNodeType']">
            <jxb:class name="CRDataNode"/>
        </jxb:bindings>

        <jxb:bindings node="//xs:complexType[@name='toAvailableWithinNodeType']">
            <jxb:class name="ToAvailableWithinNode"/>
        </jxb:bindings>

        <jxb:bindings node="//xs:complexType[@name='toAttachmentDataType']">
            <jxb:class name="toAttachmentData"/>
        </jxb:bindings>

        <jxb:bindings node="//xs:complexType[@name='attachmentsType']">
            <jxb:class name="attachments"/>
        </jxb:bindings>

        <jxb:bindings node="//xs:complexType[@name='CRAttachmentType']">
            <jxb:class name="CRAttachment"/>
        </jxb:bindings>

        <jxb:bindings node="//xs:complexType[@name='CRAttachmentDataType']">
            <jxb:class name="CRAttachmentData"/>
        </jxb:bindings>

        <!-- rename the value element -->
        <bindings node="//xs:complexType[@name='CRDataNode']">
            <bindings node=".//xs:attribute[@name='Value']">
                <property name="ValueAttribute"/>
            </bindings>
        </bindings>
    </jxb:bindings>

</jxb:bindings>